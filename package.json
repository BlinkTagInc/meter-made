{
  "name": "metermade",
  "author": "Brendan Nee <brendan@blinktag.com>",
  "version": "1.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "xo"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "basic-auth": "^2.0.1",
    "bootstrap": "^4.5.2",
    "classnames": "^2.2.6",
    "es6-promise": "^4.2.8",
    "json2csv": "^5.0.3",
    "mongoose": "^5.10.8",
    "next": "^9.5.4",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "styled-jsx-plugin-sass": "^1.0.0",
    "tsscmp": "^1.0.6"
  },
  "devDependencies": {
    "eslint-config-xo-nextjs": "^1.6.0",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "xo": "^0.33.1"
  },
  "engines": {
    "node": "12.x"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "^[A-Z]"
        }
      ],
      "semi": [
        "error",
        "never"
      ]
    }
  }
}
